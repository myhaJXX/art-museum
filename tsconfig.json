{
    "compilerOptions": {
      "target": "ES6",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react",
      "paths": {
        "@UI/*": ["./src/components/UI/*"],
        "@models/*": ["./src/models/*"],
        "@store/*": ["./src/store/*"],
        "@utils/*": ["./src/utils/*"]
      }
    },
    "include": [
      "src"
    ],
  }